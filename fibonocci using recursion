#include<stdio.h>
unsigned long long fibo(int num);
int main()
{
	int num;
	unsigned long long fibonocci;
	printf("enter a value:");
	scanf("%d",&num);
	fibonocci=fibo(num);
	printf("fibonocci of % is =%d",num,fibonocci);
	return 0;
}
unsigned long long fibo(int num)
{
	if(num==0)
	return 0;
	else if(num==1)
	return 1;
	else
	return fibo(num-1)+(num-2);
}
