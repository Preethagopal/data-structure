#include<stdio.h>
int main()
{
	int a[10],i,x,n;
	printf("how many elements in array:");
	scanf("%d",&n);
	printf("elements are:");
	for(i=0;i<n;++i)
	scanf("%d",&a[i]);
    printf("element to be searched:");
    scanf("%d",&x);
	for(i=0;i<n;++i){
    if(a[i]==x)
	break;
}
	
	if(i<n)
	printf("element at index %d",i);
	else
	printf("element not found");
}
